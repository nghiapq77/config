# Key bindings

super + Return
    $TERMINAL
super + d
    dmenu_run_i
super + r
    $TERMINAL -e ranger
super + w
    $BROWSER
super + o ; {v,h,s,m,c}
    {$TERMINAL -e vim,$TERMINAL -e htop,skypeforlinux,$TERMINAL -e neomutt,$TERMINAL -e calcurse}
super + n
    toggle_redshift.sh
super + equal
    dmenu_calc
F8
    monitor
XF86AudioMute
    amixer -D pulse set Master 1+ toggle; pkill -RTMIN+10 dwmblocks
XF86Audio{Raise,Lower}Volume
    amixer -D pulse -q set Master 5%{+,-}; pkill -RTMIN+10 dwmblocks
XF86MonBrightnessDown
    brightness down
XF86MonBrightnessUp
    brightness up
